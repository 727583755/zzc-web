            if($.trim(name).length==0){

                alert("请输入姓名");
                return false;
            }
            if($.trim(email).length==0){
                alert("请输入电子邮箱地址");
                return false;
            }
            if(!regEmail.test(email)) {

                alert("请输入的电子邮箱地址无效");
                return false;
            }

            if($.trim(method).length==0 ){

                alert("请选择购买方式");
                return false;
            }


            if($.trim(captcha_key).length==0){

                alert("请输入认证号码");
                return false;
            }

            $.ajax({
                type: "POST",
                url: "_proc.php",
                data: { mode: "confirm",key1:key1,key2:key2,name:name,email:email,method:method,captcha_key:captcha_key }
            }).done(function( msg ) {
                if(msg=="KERROR"){
                    alert("ERROR");
                    location.reload();
                }else if(msg=="ERROR"){
                    alert("ERROR");
                    location.reload();
                }else{
                    $("#contents").html(msg);
                }
            });
        }


