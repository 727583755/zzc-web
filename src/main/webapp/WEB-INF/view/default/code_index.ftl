        function ajax_fc(){
            var key1=$("#serial_one").val();
            var key2=$("#serial_two").val();
            var name=$("input[name='name']").val();
            var email=$("input[name='email']").val();
            var method=$("input[name='method']:checked").val();

            var captcha_key=$("input[name='captcha_key']").val();

